package com.modestmaps.mapproviders.google{	import com.modestmaps.core.Coordinate;	import com.modestmaps.mapproviders.IMapProvider;		/**	 * @author darren	 * $Id: GoogleRoadMapProvider.as 647 2008-08-25 23:38:15Z tom $	 */	public class GoogleTerrainMapProvider 		extends AbstractGoogleMapProvider 		implements IMapProvider	{		        public function GoogleTerrainMapProvider(minZoom:int=MIN_ZOOM, maxZoom:int=MAX_ZOOM)        {            super(minZoom, maxZoom);        }        		public function toString():String		{			return "GOOGLE_TERRAIN";		}			public function getTileUrls(coord:Coordinate):Array		{					// TODO: http://mt1.google.com/mt?v=app.81&hl=en&x=5255&s=&y=12651&z=15&s=			return [ "http://mt" + Math.floor(Math.random() * 4) + ".google.com/mt?n=404&v=" + __terrainVersion + getZoomString(sourceCoordinate(coord)) ];				}				protected function getZoomString(coord:Coordinate):String		{	        return "&x=" + coord.column + "&y=" + coord.row + "&zoom=" + (17 - coord.zoom);		}		}}